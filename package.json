    "@unocss/transformer-directives": "^0.36.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "postcss": "^8.4.14",
    "postcss-nesting": "^10.1.7",
    "typescript": "~4.7.2",
    "unocss": "^0.36.0",
